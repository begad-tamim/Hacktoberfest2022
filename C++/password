#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>
#include <time.h>
#define pass_nom 1000000
#define name_len 30
#define pass_len 9


char* passwords[pass_nom];

char numbers[] = "1234567890";
int checker (int c)
{
    int i=(c-1);

    for (i; i>=0; i--)
    {
        if (c>=1 && strcmp(passwords[c],passwords[i])==0)
        {
            return 0;
        }
    }
    return 1;
}

int main()
{
    int i=0;
    srand(time(0));// from randomize to srand
    char pass[pass_len];
    char name[name_len];
    printf("Enter your first name: ");
    fflush(stdin);
    fgets(name,name_len+1,stdin);
    for (i; i<=strlen(name); i++)
    {
        name[i] = tolower(name[i]);
    }
    int j2 = -1;

    for (int c=0; c<pass_nom; c++)
    {
        memset(pass,0,pass_len);

        for (int i=0; i<(pass_len - (strlen(name)-1)); i++)
        {
            pass[i] = numbers[rand()%10];//from randomize to rand

        }
        strcat(pass,name);
        for (int i=0; i<pass_len; i++)
        {
            int j = rand()%pass_len;
            if (j2 != j)
            {
                j2 = j;
                int t = pass[j];
                pass[j] = pass[i];
                pass[i] = t;
            }
            else
            {

                i--;
            }
        }
        printf("%s\n",pass);
    }
    return 0;
}
